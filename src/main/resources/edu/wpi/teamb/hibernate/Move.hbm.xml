<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.wpi.teamb.Database.Move" table="move">
        <id name="nodeID" type="String" column="node">
        </id>
        <property name = "locationName" type = "String">
            <column name="longname"/>
        </property>
        <property name = "moveDate" type = "java.sql.Date">
            <column name="movedate"/>
        </property>
    </class>
</hibernate-mapping>