<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.wpi.teamb.Database.SanitationRequest" table="sanitationrequest">
        <property name = "cleanUpLocation" type = "String">
            <column name="cleanuplocation"/>
