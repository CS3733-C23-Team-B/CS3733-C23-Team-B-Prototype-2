                     <children>
                        <MFXFilterComboBox id="button" fx:id="locationBox" floatMode="DISABLED" maxHeight="40.0" maxWidth="1.7976931348623157E308" minHeight="40.0" onAction="#setFields" prefHeight="40.0" prefWidth="153.0" style="-fx-background-color: #eeeeee; -fx-border-color: #cecece; -fx-background-radius: 8; -fx-border-radius: 8;" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" prefWidth="287.0" spacing="10.0">
               <children>
                  <Label id="mapEditorPopupBodySmall" maxHeight="1.7976931348623157E308" text="Long Name" textAlignment="CENTER" textFill="#333333" wrapText="true" />
                  <HBox alignment="CENTER_RIGHT" HBox.hgrow="ALWAYS">
                     <children>
                        <MFXTextField id="button" fx:id="longNameField" floatMode="DISABLED" maxHeight="40.0" minHeight="40.0" prefHeight="40.0" prefWidth="153.0" style="-fx-background-color: #eeeeee; -fx-border-color: #cecece; -fx-background-radius: 8; -fx-border-radius: 8;" />
