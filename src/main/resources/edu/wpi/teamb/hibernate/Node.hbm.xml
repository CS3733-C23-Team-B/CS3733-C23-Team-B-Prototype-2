<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.wpi.teamb.Database.Node" table="node">
        <id name="NodeID" type="String" column = "node">
        </id>
        <property name = "XCoord" type = "int">
            <column name="xCoord"/>
        </property>
        <property name = "YCoord" type = "int">
            <column name="yCoord"/>
        </property>
        <property name = "floor" type = "String">
            <column name="Floor"/>
        </property>
        <property name = "Building" type = "String">
            <column name="Building"/>
        </property>
    </class>
</hibernate-mapping>