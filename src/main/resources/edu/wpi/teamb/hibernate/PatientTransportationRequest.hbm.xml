<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.wpi.teamb.Entities.PatientTransportationRequest" table="patienttransportationrequest">
        <property name = "equipmentNeeded" type = "String">
            <column name="equipmentNeeded"/>
        </property>
        <property name = "patientLocation" type = "String">
            <column name="patientLocation"/>
        </property>
        <property name = "patientDestination" type = "String">
            <column name="patientDestination"/>
        </property>
        <property name = "patientID" type = "String">
            <column name="patientID"/>
        </property>
        <property name = "patientID" type = "String">
            <column name="patientID"/>
        </property>
    </class>
</hibernate-mapping>