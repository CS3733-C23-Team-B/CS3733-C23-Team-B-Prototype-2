<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="edu.wpi.teamb.Database.LocationName" table="locationname">
        <id name="longname" type="String" column="longname">
        </id>
        <property name = "shortName" type = "String">
            <column name="shortName"/>
        </property>
        <property name = "locationType" type = "String">
            <column name="locationType"/>
        </property>
    </class>
</hibernate-mapping>