                     <children>
                        <MFXFilterComboBox id="button" fx:id="nodeBox" floatMode="DISABLED" maxHeight="40.0" maxWidth="1.7976931348623157E308" minHeight="40.0" prefHeight="40.0" prefWidth="153.0" style="-fx-background-color: #eeeeee; -fx-border-color: #cecece; -fx-background-radius: 8; -fx-border-radius: 8;" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="5.0" top="5.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
      <HBox alignment="CENTER" spacing="50.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
         <children>
            <MFXButton id="mapEditorPopupButton" layoutX="99.0" layoutY="60.0" minHeight="46.0" minWidth="100.0" onAction="#deleteClicked" style="-fx-background-color: #E89F55; -fx-background-radius: 5;" text="Delete" textFill="WHITE">
